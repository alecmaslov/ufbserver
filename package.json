  "scripts": {
    "start": "tsx watch src/app.ts",
    "build": "tsc -p .",
    "kyle-dev": "API_PORT=4953 tsx watch src/app.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "tsc -w",
    "generate-schema": "npx schema-codegen src/game/schema/* --output data/schema --csharp --namespace UFB.StateSchema",
    "generate-schema-client": "npx schema-codegen src/game/schema/* --output ../ufb_unity/Assets/Scripts/Network/Schema --csharp --namespace UFB.StateSchema",
    "job:insert-maps": "node out/jobs/insert-maps.js",
    "job:update-characters": "node out/jobs/update-characters.js",
    "start:service": "pm2 stop ufb-server; pm2 start out/app.js --name ufb-server",
    "stop:service": "pm2 stop ufb-server"
  },
